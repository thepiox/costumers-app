{
	"name": "customers-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"ddbb": "json-server --watch --port-=4000 DDBB.json ",
		"lint": "eslint src/**/*.js",
		"pretty": "prettier --write \"./src/**/*.{js,jsx,json}\""
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn pretty && yarn lint"
		}
	},
	"dependencies": {
		"axios": "^0.21.1",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-redux": "^7.1.3",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.2.0",
		"redux": "^4.0.4",
		"redux-actions": "^2.6.5",
		"redux-promise": "^0.6.0"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"eslint": "^6.6.0",
		"husky": "^3.1.0",
		"json-server": "^0.15.1",
		"prettier": "^1.19.1"
	}
}
