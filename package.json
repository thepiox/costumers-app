		}
	},
	"dependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-scripts": "3.2.0",
		"axios": "^0.19.0",
		"prop-types": "^15.7.2"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"husky": "^3.1.0",
		"prettier": "^1.19.1",
		"eslint": "^6.6.0"
	}
}
